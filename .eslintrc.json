{
    "env": {
        "node": true,
        "commonjs": true,
        "es2020": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-console": "off",
        "prettier/prettier": "error"
    }
}
